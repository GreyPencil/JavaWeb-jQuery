<%@ page contentType="text/html;charset=UTF-8" language="java" %>

<%
    String basePath = request.getScheme()
        + "://"
        + request.getServerName()
        + ":"
        + request.getServerPort()
        + request.getContextPath()
        + "/";

    pageContext.setAttribute("basePath", basePath);
%>
<%--<%=basePath%>--%>
<base href="<%=basePath%>">
